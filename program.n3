@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix http: <http://www.w3.org/2011/http#> .
@prefix httpm: <http://www.w3.org/2011/http-methods#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix arena: <http://arena2036.example.org/> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix : <http://127.0.1.1:8080/> .

{
    [] http:mthd httpm:GET ;
        http:requestURI :transporter2 .
}

{
    ?transporter a arena:Transporter ;
        arena:queue ?queue .
} => {
    [] http:mthd httpm:GET ;
        http:requestURI ?queue .
} .

{
    ?transporter a arena:Transporter ;
        arena:locationX "30"^^xsd:integer ;
        arena:queue ?queue ;
        arena:tasks rdf:nil .
} => {
    [] http:mthd httpm:PUT ;
        http:requestURI ?queue ;
        http:body {
            ?transporter arena:tasks _:l .

            _:l rdf:first :transporterTask4 ;
                rdf:rest rdf:nil .
        } .
} .

{
    ?transporter a arena:Transporter ;
        arena:queue ?queue ;
        arena:locationX "30"^^xsd:integer ;
        arena:tasks rdf:nil .
} => {
    [] http:mthd httpm:POST ;
        http:requestURI :transporterTask4 ;
        http:body {
            :transporterTask4 a arena:TransporterTask ;
                arena:instrument :transporter2 ;
                arena:toLocationX "34"^^xsd:integer ;
                arena:toLocationY "11"^^xsd:integer ;
                arena:input :product6 .
        } .
} .

{
    ?transporter a arena:Transporter ;
        arena:locationX "34"^^xsd:integer ;
        arena:queue ?queue ;
        arena:tasks rdf:nil .
} => {
    [] http:mthd httpm:PUT ;
        http:requestURI ?queue ;
        http:body {
            ?transporter arena:tasks _:l .

            _:l rdf:first :transporterTask5 ;
                rdf:rest rdf:nil .
        } .
} .

{
    ?transporter a arena:Transporter ;
        arena:queue ?queue ;
        arena:locationX "34"^^xsd:integer ;
        arena:tasks rdf:nil .
} => {
    [] http:mthd httpm:POST ;
        http:requestURI :transporterTask5 ;
        http:body {
            :transporterTask5 a arena:TransporterTask ;
                arena:instrument :transporter2 ;
                arena:toLocationX "36"^^xsd:integer ;
                arena:toLocationY "12"^^xsd:integer .
        } .
} .